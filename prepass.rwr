% rewrite gridprepass {
  main [stuff+] = ‛«stuff»’
  stuff [x] = ‛«x»’
  keyword [kw] = ‛⎨downcase ‛«kw»’⎬’

  string [dqL cs* dqR] = ‛"«cs»"’
  strchar_escapedquote [dq1 dq2] = ‛\"’
  strchar_other [c] = ‛«c»’
    
  ident [c tailcs*] = ‛⎨downcase ‛«c»«tailcs»’⎬’
  idtail [c] = ‛«c»’
  comment [q cs* nl] = ‛⎝«cs»⎠  ⎨incnl⎬\n’
  nl [c] = ‛  ⎨incnl⎬\n’
}
